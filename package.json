  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "copy next.config.hybrid.mjs next.config.mjs && next dev && git checkout next.config.mjs",
    "dev:web": "next dev",
    "dev:mobile": "copy next.config.hybrid.mjs next.config.mjs && next dev & npx cap run android --livereload",
    "dev:mobile-prod": "copy capacitor.config.mobile.ts capacitor.config.ts && copy next.config.hybrid.mjs next.config.mjs && npx cap run android --livereload",
    "build": "next build",
    "build:capacitor": "rmdir /s /q app\\api && copy next.config.capacitor.mjs next.config.mjs && next build && git checkout next.config.mjs && git checkout app\\api",
    "build:mobile": "copy next.config.mobile.mjs next.config.mjs && next build && npx cap sync && git checkout next.config.mjs",
    "build:mobile-prod": "copy capacitor.config.prod.ts capacitor.config.ts && next build && npx cap sync && git checkout capacitor.config.ts",
    "build:mobile-api": "copy capacitor.config.prod.ts capacitor.config.ts && next build && npx cap sync && git checkout capacitor.config.ts",
    "build:android-prod": "cd android && .\\gradlew assembleRelease",
    "build:android-bundle": "cd android && .\\gradlew bundleRelease",
    "deploy:android": "npm run build:mobile-prod && npm run build:android-prod",
    "deploy:android-bundle": "npm run build:mobile-prod && npm run build:android-bundle",
    "mobile:android": "npx cap open android",
    "mobile:ios": "npx cap open ios",
    "mobile:start": "node mobile-app/server.js",
    "mobile:sync": "npx cap sync",
    "mobile:sync-prod": "copy capacitor.config.mobile.ts capacitor.config.ts && npx cap sync && git checkout capacitor.config.ts",
    "mobile:sync-api": "copy capacitor.config.prod.ts capacitor.config.ts && npx cap sync && git checkout capacitor.config.ts",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "backup": "node scripts/backup-database.js",
    "cleanup-backups": "node scripts/cleanup-backups.js",
    "schedule-backups": "node scripts/schedule-backups.js",
    "test-backup": "node scripts/test-schedule.js",
    "init-achievements": "node scripts/init-achievements.js",
    "restore-backup": "node scripts/restore-backup.js",
    "debug-streak": "node scripts/debug-streak.js",
    "recalculate-streak": "node scripts/recalculate-streak.js",
    "check-date-habits": "node scripts/check-date-habits.js",

{
  "name": "productif.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cp next.config.hybrid.mjs next.config.mjs && next dev && git checkout next.config.mjs",
    "dev:web": "next dev",
    "dev:mobile": "cp next.config.hybrid.mjs next.config.mjs && next dev & npx cap run android --live-reload",
    "dev:mobile-prod": "cp capacitor.config.mobile.ts capacitor.config.ts && cp next.config.hybrid.mjs next.config.mjs && npx cap run android --live-reload",
    "dev:whatsapp": "nodemon src/index.js",
    "dev:scheduler": "nodemon src/services/scheduler-service.js",
    "schedule-morning": "node src/scripts/schedule-morning-reminders.js",
    "build": "npx prisma generate && next build",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "build:capacitor": "rm -rf app/api && cp next.config.capacitor.mjs next.config.mjs && next build && git checkout next.config.mjs && git checkout app/api",
    "build:mobile": "cp next.config.mobile.mjs next.config.mjs && next build && npx cap sync && git checkout next.config.mjs",
    "build:mobile-prod": "cp capacitor.config.prod.ts capacitor.config.ts && next build && npx cap sync && git checkout capacitor.config.ts",
    "build:mobile-api": "cp capacitor.config.prod.ts capacitor.config.ts && next build && npx cap sync && git checkout capacitor.config.ts",
    "build:android-prod": "cd android && ./gradlew assembleRelease",
    "build:android-bundle": "cd android && ./gradlew bundleRelease",
    "deploy:android": "npm run build:mobile-prod && npm run build:android-prod",
    "deploy:android-bundle": "npm run build:mobile-prod && npm run build:android-bundle",
    "mobile:android": "npx cap open android",
    "mobile:ios": "npx cap open ios",
    "mobile:start": "node mobile-app/server.js",
    "mobile:sync": "npx cap sync",
    "mobile:sync-prod": "cp capacitor.config.mobile.ts capacitor.config.ts && npx cap sync && git checkout capacitor.config.ts",
    "mobile:sync-api": "cp capacitor.config.prod.ts capacitor.config.ts && npx cap sync && git checkout capacitor.config.ts",
    "start": "next start",
    "start:whatsapp": "node src/index.js",
    "start:scheduler": "node src/services/scheduler-service.js",
    "start:ai": "tsx src/services/ai/start.ts",
    "test:notifications": "node src/tests/notification-test-v2.js",
    "test:setup": "node src/scripts/create-test-preferences.js",
    "lint": "next lint",
    "backup": "node scripts/backup-database.js",
    "backup:complete": "node scripts/backup-complete.js",
    "backup:gamification": "node scripts/backup-gamification-waitlist.js",
    "cleanup-backups": "node scripts/cleanup-backups.js",
    "schedule-backups": "node scripts/schedule-backups.js",
    "test-backup": "node scripts/test-schedule.js",
    "init-achievements": "node scripts/init-achievements.js",
    "restore-backup": "node scripts/restore-backup.js",
    "debug-streak": "node scripts/debug-streak.js",
    "recalculate-streak": "node scripts/recalculate-streak.js",
    "check-date-habits": "node scripts/check-date-habits.js",
    "fix-habit-dates": "node scripts/fix-habit-dates.js",
    "analyze-date-flow": "node scripts/analyze-date-flow.js",
    "add-waitlist": "ts-node -r tsconfig-paths/register scripts/add-waitlist-entries.ts"
  },

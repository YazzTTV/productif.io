{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "app/api/dashboard/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/analytics/**/*.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    { 
      "src": "/.well-known/assetlinks.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    { "src": "/fonctionnalites", "dest": "/app/(marketing)/fonctionnalites" },
    { "src": "/tarifs", "dest": "/app/(marketing)/tarifs" },
    { "src": "/tarification", "dest": "/app/(marketing)/tarification" }
  ]
} 